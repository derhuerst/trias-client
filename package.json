{
    "name": "trias-client",
    "version": "0.6.0-alpha.0",
    "description": "Lean Node.js client for public transport TRIAS APIs.",
    "keywords": [
        "public-transport",
        "trias"
    ],
    "main": "lib/index.js",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint .",
        "test": "tsc && jest"
    },
    "jest": {
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ],
        "verbose": true
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andaryjo/trias-client.git"
    },
    "author": "Andary",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/andaryjo/trias-client/issues"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.168",
        "@types/node": "^14.14.35",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jest": "^24.3.6",
        "friendly-public-transport-format": "^1.2.1",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.3"
    },
    "dependencies": {
        "@types/xmldom": "^0.1.30",
        "axios": "^0.21.1",
        "css-select": "^4.1.2",
        "domhandler": "^4.2.0",
        "htmlparser2": "^6.1.0",
        "lodash": "^4.17.21",
        "moment-timezone": "^0.5.33"
    },
    "files": [
        "lib/**/*"
    ]
